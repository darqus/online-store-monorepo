{
  "name": "online-store",
  "version": "1.0.0",
  "description": "online store",
  "license": "ISC",
  "author": "darqus",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec \"node --env-file=.env index.js\"",
    "start": "node index.js",
    "kill": "pkill -f node",
    "lint": "eslint .",
    "format": "prettier -w .",
    "migrate": "node db/run-migrations.js up",
    "migrate:down": "node db/run-migrations.js down",
    "seed": "node db/run-seeders.js up",
    "seed:down": "node db/run-seeders.js down",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --runInBand",
    "update-deps": "npm update && npm audit fix && npm test",
    "db:migrate": "npm run migrate",
    "db:migrate:down": "npm run migrate:down",
    "db:seed": "npm run seed",
    "db:seed:down": "npm run seed:down"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^5.1.1",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pino": "^9.3.2",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.6.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "umzug": "^3.8.1"
  }
}
